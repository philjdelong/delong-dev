<div class='description'>
	<h1>Profile:</h1>
	<h3>username</h3>
	<p><%= current_user.username %></p>
	<h3>email</h3>
	<p><%= current_user.email %></p>
		<br>
	<% if current_user.admin? %>
		<h3>Registered Users (<%= @users.count - 1 %>)</h3>
		<% @users.each do |user| %>
			<% unless user == current_user %>
				<h1><section><%= user.username %></section></h1>
				<p><section>email: <%= user.email %><section></p>
			<% end %>
		<% end %>
		<br>
	<% end %>
</div>
